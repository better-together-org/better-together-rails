<div id="<%= dom_id(journey_map, :partners) %>" class="journey-map-partners" data-journey-map-target="partners">
  <h4>Partners</h4>
  <% if partners.any? %>
    <%= render partial: 'journey_maps/partner', collection: partners, locals: { journey_map: journey_map } %>
  <% elsif @topic %>
    <p>No <%= topic.name %> partners available yet.</p>
  <% else %>
    <p>No topic partners available yet.</p>
  <% end %>
</div>